					<Anchor point="LEFT" relativeTo="$parent_receiveBar" relativePoint="RIGHT">
						<Offset x="5" y="0" />
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>self:Disable()</OnLoad>
					<OnClick>sdm_CancelReceive()</OnClick>
				</Scripts>
			</Button>
